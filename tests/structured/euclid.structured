begin
   var a = 1409286144;    // 7   *     3   * 2^26
   var b = 6615;          // 7^2 * 5 * 3^3
   // The result will be:    7   *     3          = 21
   while a <> b do
      if a < b then
         b := b - a;
      else
         a := a - b;
      end;
   end;
   print a;
end

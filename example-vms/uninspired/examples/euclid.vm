        mov 3333333333, %r0
        mov 1, %r1

$loop:
        beq %r0, %r1, $done
        blt %r0, %r1, $swap
        sub %r0, %r1, %r0
        b $loop
$swap:
        mov %r1, %r2
        mov %r0, %r1
        mov %r2, %r0
        b $loop
$done:
        printfixnum %r1

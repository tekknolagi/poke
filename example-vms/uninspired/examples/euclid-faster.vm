        mov 3333333333, %r0
        mov 1, %r1

        b $check

$r0-bigger:
        sub %r0, %r1, %r0
$check:
        bgt %r0, %r1, $r0-bigger
        beq %r0, %r1, $done

$r0-smaller:
        sub %r1, %r0, %r1
        blt %r0, %r1, $r0-smaller
        bne %r0, %r1, $r0-bigger

$done:
        printfixnum %r1
